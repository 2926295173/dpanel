{
  "name": "dpanel-plugin-explorer",
  "imageName": "dpanel/explorer",
  "image": {
    "amd64": "ccr.ccs.tencentyun.com/library/alpine:latest",
    "arm64": "ccr.ccs.tencentyun.com/library/alpine:latest"
  },
  "env": {
    "pidHost": true,
    "privileged": true,
    "restart": "always",
    "autoRemove": true
  },
  "container": {
    "init": "sed -i 's/dl-cdn.alpinelinux.org/mirrors.tuna.tsinghua.edu.cn/g' /etc/apk/repositories && apk add file"
  }
}